{
    "name": "kevin-verschaeve/var-trumper",
    "type": "library",
    "description": "Symfony mechanism for exploring and dumping PHP variables",
    "keywords": ["dump", "debug", "trump"],
    "homepage": "https://symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Grekas",
            "email": "p@tchwork.com"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://symfony.com/contributors"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/polyfill-mbstring": "~1.0"
    },
    "require-dev": {
        "twig/twig": "~1.20|~2.0"
    },
    "suggest": {
        "ext-symfony_debug": ""
    },
    "autoload": {
        "files": [ "Resources/functions/trump.php" ],
        "psr-4": { "VarTrumper\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev"
}
